{
  "name": "sveltekit",
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:migrate": "drizzle-kit migrate",
    "db:drop": "drizzle-kit drop",
    "db:seed": "vite-node --options.transformMode.ssr='/.*/' src/lib/db/seed",
    "db:clear": "vite-node --options.transformMode.ssr='/.*/' src/lib/db/clear",
    "db:studio": "drizzle-kit studio",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "test:e2e": "pnpm db:clear && NODE_ENV=test playwright test",
    "test:unit": "vitest",
    "test": "pnpm test:e2e && pnpm test:unit",
    "prepare": "husky"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.0",
    "@eslint/js": "^9.37.0",
    "@faker-js/faker": "^10.1.0",
    "@lucide/svelte": "^0.562.0",
    "@playwright/test": "^1.57.0",
    "@sveltejs/adapter-auto": "^6.1.1",
    "@sveltejs/kit": "^2.49.2",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/table-core": "^8.21.3",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/svelte": "^5.3.1",
    "@types/eslint": "^9.6.1",
    "@types/node": "^25.0.2",
    "bits-ui": "2.14.4",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.8",
    "embla-carousel-svelte": "^8.6.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.13.1",
    "formsnap": "2.0.1",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "layerchart": "2.0.0-next.43",
    "lint-staged": "^16.2.7",
    "mode-watcher": "^1.1.0",
    "paneforge": "1.0.2",
    "prettier": "^3.7.4",
    "prettier-plugin-svelte": "^3.4.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "svelte": "5.46.1",
    "svelte-check": "^4.3.4",
    "svelte-sonner": "^1.0.7",
    "sveltekit-flash-message": "^2.4.6",
    "sveltekit-rate-limiter": "^0.7.0",
    "sveltekit-superforms": "^2.29.1",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "vaul-svelte": "1.0.0-next.7",
    "vite": "^7.3.0",
    "vite-node": "^5.2.0",
    "vitest": "^3.2.4",
    "zod": "^4.2.1"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.940.0",
    "@aws-sdk/s3-request-presigner": "^3.922.0",
    "@libsql/client": "^0.15.15",
    "@node-rs/argon2": "^2.0.2",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@pilcrowjs/object-parser": "^0.0.4",
    "arctic": "^3.7.0",
    "drizzle-orm": "^0.45.1",
    "mjml": "^4.15.3",
    "nanoid": "^5.1.6",
    "resend": "^6.5.2",
    "sailkit": "^0.3.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "esbuild"
    ]
  },
  "lint-staged": {
    "*": "pnpm lint"
  }
}
