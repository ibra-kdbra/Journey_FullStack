{
	"$schema": "../node_modules/@tauri-apps/cli/schema.json",
	"productName": "tauri-sveltekit",
	"version": "0.1.0",
	"identifier": "com.auros.tauri-sveltekit",
	"build": {
		"beforeBuildCommand": "npm run sveltekit:build",
		"beforeDevCommand": "npm run sveltekit:dev",
		"devUrl": "http://localhost:5173",
		"frontendDist": "../build"
	},
	"app": {
		"windows": [
			{
				"title": "Tauri x SvelteKit",
				"width": 800,
				"height": 600,
				"resizable": true,
				"fullscreen": false
			}
		],
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"category": "DeveloperTool",
		"copyright": "",
		"shortDescription": "",
		"longDescription": "",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		],
		"resources": [],
		"externalBin": [],
		"linux": {
			"deb": {
				"depends": []
			}
		},
		"macOS": {
			"frameworks": [],
			"entitlements": null,
			"exceptionDomain": "",
			"providerShortName": null,
			"signingIdentity": null
		},
		"windows": {
			"certificateThumbprint": null,
			"digestAlgorithm": "sha256",
			"timestampUrl": ""
		}
	}
}
