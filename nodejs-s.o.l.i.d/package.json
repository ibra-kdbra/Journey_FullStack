{
  "name": "refactor-node",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --watch './**/*.ts' --exec 'ts-node' ./src/index.ts",
    "migrate:up": "npx sequelize-cli db:migrate",
    "migrate:down": "npx sequelize-cli db:migrate:undo",
    "seed:up": "npx sequelize-cli db:seed:all",
    "seed:down": "npx sequelize-cli --config config/config.js db:seed:undo:all",
    "clean": "rimraf dist/",
    "build": "yarn clean && npx tsc",
    "start": "yarn build && nodemon dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "dotenv": "^17.0.0",
    "exceljs": "^4.3.0",
    "express": "^5.1.0",
    "joi": "^18.0.0",
    "mysql2": "^3.3.5",
    "nodemailer": "^7.0.2",
    "pdfmake": "^0.2.7",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.32.0",
    "sequelize-cli": "^6.6.1",
    "sequelize-typescript": "^2.1.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.0",
    "@types/node": "^24.0.1",
    "@types/nodemailer": "^6.4.8",
    "@types/pdfmake": "^0.2.2",
    "nodemon": "^3.1.9",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  }
}